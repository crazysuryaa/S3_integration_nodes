# S3 Integration Environment Variables
# Copy this file to .env and fill in your actual values

# Required S3 Configuration
S3_REGION=us-east-1
S3_ACCESS_KEY=your_access_key_here
S3_SECRET_KEY=your_secret_key_here
S3_BUCKET_NAME=your_bucket_name_here

# Optional: Custom S3 endpoint (leave empty for AWS)
# S3_ENDPOINT_URL=https://s3.amazonaws.com

# Optional: Logging Configuration
# S3_LOG_LEVEL=INFO
# S3_LOG_FORMAT=simple
# S3_LOG_TO_FILE=false
# S3_LOG_FILE_PATH=logs/s3_integration.log

# Optional: Custom directories within your bucket
# S3_INPUT_DIR=input
# S3_OUTPUT_DIR=output